function f = objectiveFunc(x,winProbs,expectations,variances,bettingPools,TeamPick,theta,deterministic,chanceconstrained)

bettingPools = bettingPools;

TeamPick = TeamPick;

%probability of a team winning over another team, deterministic
winProbs = winProbs;

%expected value of a team winning over another team and the variance
expectations = expectations;
variances = variances;

%what has happened is that the probability winProb are dependent on 
%the picked team which is currently randomly generated between 1 and 2 as
%such your "cut" is also dependent on your picked team

if deterministic = true
    f = (-1*winProbs(1,TeamPick(1))*(x(1)+bettingPools(1,1)+x(2)+bettingPools(1,2))*...
            (x(TeamPick(1))/(x(TeamPick(1))+bettingPools(1,TeamPick(1)))))+...      
        (-1*winProbs(2,TeamPick(2))*(x(3)+bettingPools(2,1)+x(4)+bettingPools(2,2))*...
            (x(TeamPick(2)+2)/(x(TeamPick(2)+2)+bettingPools(2,TeamPick(2)))))+...
        (-1*winProbs(3,TeamPick(3))*(x(5)+bettingPools(3,1)+x(6)+bettingPools(3,2))*...
            (x(TeamPick(3)+4)/(x(TeamPick(3)+4)+bettingPools(3,TeamPick(3)))))+...
        (-1*winProbs(4,TeamPick(4))*(x(7)+bettingPools(4,1)+x(8)+bettingPools(4,2))*...
            (x(TeamPick(4)+6)/(x(TeamPick(4)+6)+bettingPools(4,TeamPick(4)))))+...
        (-1*winProbs(5,TeamPick(5))*(x(9)+bettingPools(5,1)+x(10)+bettingPools(5,2))*...
            (x(TeamPick(5)+8)/(x(TeamPick(5)+8)+bettingPools(5,TeamPick(5)))))+...
        (-1*winProbs(6,TeamPick(6))*(x(11)+bettingPools(6,1)+x(12)+bettingPools(6,2))*...
            (x(TeamPick(6)+10)/(x(TeamPick(6)+10)+bettingPools(6,TeamPick(6)))));
elseif chanceconstrained = true;
        (-1*expectations(1,TeamPick(1))*(x(1)+bettingPools(1,1)+x(2)+bettingPools(1,2))*...
            (x(TeamPick(1))/(x(TeamPick(1))+bettingPools(1,TeamPick(1)))))+...      
        (-1*expectations(2,TeamPick(2))*(x(3)+bettingPools(2,1)+x(4)+bettingPools(2,2))*...
            (x(TeamPick(2)+2)/(x(TeamPick(2)+2)+bettingPools(2,TeamPick(2)))))+...
        (-1*expectations(3,TeamPick(3))*(x(5)+bettingPools(3,1)+x(6)+bettingPools(3,2))*...
            (x(TeamPick(3)+4)/(x(TeamPick(3)+4)+bettingPools(3,TeamPick(3)))))+...
        (-1*expectations(4,TeamPick(4))*(x(7)+bettingPools(4,1)+x(8)+bettingPools(4,2))*...
            (x(TeamPick(4)+6)/(x(TeamPick(4)+6)+bettingPools(4,TeamPick(4)))))+...
        (-1*expectations(5,TeamPick(5))*(x(9)+bettingPools(5,1)+x(10)+bettingPools(5,2))*...
            (x(TeamPick(5)+8)/(x(TeamPick(5)+8)+bettingPools(5,TeamPick(5)))))+...
        (-1*expectations(6,TeamPick(6))*(x(11)+bettingPools(6,1)+x(12)+bettingPools(6,2))*...
            (x(TeamPick(6)+10)/(x(TeamPick(6)+10)+bettingPools(6,TeamPick(6)))))+...
            theta*sqrt(variances(1,1)*x(1)^2;
end

%     -1*R(2)*(x(3)+w3+x(4)+w4)*(x(3)/(x(3)+w3))+...
%     -1*R(3)*(x(5)+w5+x(6)+w6)*(x(5)/(x(5)+w5))+...
%     -1*R(3)*(x(7)+w7+x(8)+w8)*(x(7)/(x(7)+w7))+...
%     -1*R(3)*(x(9)+w9+x(10)+w10)*(x(9)/(x(9)+w9))+...
%     -1*R(3)*(x(11)+w11+x(12)+w12)*(x(11)/(x(11)+w11));

